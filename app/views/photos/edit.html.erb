<h1> Edit Photo </h1>

<form action="/update_photo/<%= @p.id %>">
  <div>
    <label>Caption:</label>
    <input id="Caption" type="text" name="the_caption" value="<%= @p.caption %>">

  </div>
  <div>
    <label>Image URL:</label>
    <input id="Image URL" type="text" name="the_source" value="<%= @p.source %>">
  </div>
  <div>
    <button>Update Photo</button>
  </div>
</form>
